<h1>Find Student data is <%= customers.length %>  
    <table border="1">
        <tr>
            <th>name</th>
            <th>lname</th>

        </tr>
        <% for(var i=0; i < JSON.stringify(customers.length); i++) { %>
            <tr>
                
                <td>
                    <%= customers[i].name %>
                </td>
                <td>
                    <%= customers[i].lname %>
                </td>
                
            </tr>
            <% } %>
    </table>

</h1>